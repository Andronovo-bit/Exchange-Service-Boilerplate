  "main": "dist/app.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "tsx ./src/app.ts",
    "dev": "nodemon",
    "build": "npx tsc",
    "migrate:generate": "npx sequelize-cli migration:generate --name",
