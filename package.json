    "migrate:undo:all": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed --seed",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "sequelize": "npx sequelize-cli"
